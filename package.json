{
  "name": "typescript-template",
  "version": "2.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/theS1LV3R/typescript-template",
  "issues": "https://github.com/theS1LV3R/typescript-template/issues",
  "author": "S1LV3R <s1lv3r@corax.team>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "dev:inspect": "cross-env NODE_ENV=development nodemon --exec node --trace-warnings --inspect --require ts-node/register src/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "version": "standard-version",
    "postversion": "echo \"Remember to sign any tags!\"",
    "commit": "commit",
    "prepare": "husky install",
    "test": "jest --detectOpenHandles --colors --verbose --reporters=default",
    "test:ci": "jest --ci --detectOpenHandles --colors --verbose --reporters=default",
    "docs": "typedoc src"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@commitlint/prompt-cli": "13.2.1",
    "@types/jest": "27.0.2",
    "@types/node": "15.3.1",
    "@typescript-eslint/eslint-plugin": "5.0.0",
    "@typescript-eslint/parser": "5.0.0",
    "cross-env": "7.0.3",
    "eslint": "8.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "husky": "7.0.2",
    "jest": "27.2.5",
    "nodemon": "2.0.13",
    "prettier": "2.4.1",
    "standard-version": "9.3.1",
    "ts-jest": "27.0.6",
    "ts-node": "10.3.0",
    "typedoc": "0.22.5",
    "typescript": "4.4.4"
  }
}
